**자가진단**
1)
#include <stdio.h>
int main(void)
{
    char a[100];
    int i, b[26] = { 0 };
    for (i = 0; i < 100; i++) {
        scanf(" %c", &a[i], sizeof(a[i]));
        b[a[i] - 65]++;
        if (a[i] < 65 || a[i]>90)
            break;
    }
    for (i = 0; i < 26; i++) {
        if (b[i] > 0) {
            printf("%c : %d\n", 'A' + i, b[i]);
        }
    }
    return 0;
}


2)
#include <stdio.h>
int main(void)
{
    int a[100], b[10] = { 0 }, i;
    for (i = 0; i < 100; i++) {
        scanf("%d", &a[i]);
        if (a[i] == 0)
            break;
        b[a[i] / 10]++;
    }
    for (i = 0; i < 10; i++) {
        if (b[i] > 0)
            printf("%d : %d\n", i, b[i]);
    }
    return 0;
}


3)
#include <stdio.h>
int main(void)
{
    int a[100], i;
    a[0] = 100;
    scanf("%d", &a[1]);
 
    printf("%d %d ", a[0], a[1]);
    for (i = 0; i < 100; i++) {
        a[i + 2] = a[i] - a[i+1];
        printf("%d ", a[i + 2]);
        if (a[i + 2] < 0)
            break;
    }
    return 0;
}


4)
#include <stdio.h>
int main(void)
{
    int a[3][5] = {
        {5,8,10,6,4}, {11,20,1,13,2}, {7,9,14,22,3}
    };
 
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 5; j++) {
            printf("%2d   ", a[i][j]);
        }
        printf("\n");
    }
    return 0;
}


5)
#include <stdio.h>
int main(void)
{
    int a[2][4] = { 0 };
    int b[2][4] = { 0 };
    int i, j;
 
    printf("first array\n");
    for (i = 0; i < 2; i++) {
        for (j = 0; j < 4; j++) {
            scanf("%d", &a[i][j]);
        }
    }
    printf("second array\n");
    for (i = 0; i < 2; i++) {
        for (j = 0; j < 4; j++) {
            scanf("%d", &b[i][j]);
        }
    }
    for (i = 0; i < 2; i++) {
        for (j = 0; j < 4; j++) {
            printf("%d ", a[i][j] * b[i][j]);
        }
        printf("\n");
    }
    return 0;
}


6)
#include <stdio.h>
int main(void)
{
    int a[10], b;
    for (int i = 0; i < 10; i++) {
        scanf("%d", &a[i]);
    }
    b = a[0];
     
    for (int i = 0; i < 10; i++) {
        if (b > a[i])
            b = a[i];
    }
    printf("%d", b);
    
    return 0;
}


7)
#include <stdio.h>
int main(void)
{
    int a[5][4] = { 0 }, sum[5] = { 0 };
    int i, j, count = 0;
    for (i = 0; i < 5; i++) {
        for (j = 0; j < 4; j++) {
            scanf("%d", &a[i][j]);
            sum[i] += a[i][j];
        }
    }
 
    for (i = 0; i < 5; i++) {
        if (sum[i] / 4 >= 80) {
                printf("pass\n");
                count++;
        }
        else
            printf("fail\n");
 
    }
    printf("Successful : %d\n\n", count);
    
    return 0;
}


**형성평가**
1)
#include <stdio.h>
int main(void) {
    int a[10], b[6] = { 0 };
    int i;
    for (i = 0; i < 10; i++) {
        scanf("%d", &a[i]);
        b[a[i]-1]++;
    }
    for (i = 0; i < 6; i++) {
        printf("%d : %d\n", i + 1, b[i]);
    }
    return 0;
}


2)
#include <stdio.h>
int main()
{
    int a[11] = { 0 };
    int i, num;
    for (i = 0; i < 100; i++) {
        scanf("%d", &num);
        if (num == 0) break;
        a[num / 10]++;
    }
    for (i = 10; i >= 0; i--) {
        if (a[i] > 0) {
            printf("%d : %d person \n", i * 10, a[i]);
        }
    }
    return 0;
}


