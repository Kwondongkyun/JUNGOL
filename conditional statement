**자가진단**
1)
#include <stdio.h>
int main(void)
{
    int a;
    scanf("%d", &a);
 
    printf("%d\n", a);
 
    if (a < 0)
    {
        printf("minus");
    }
 
    return 0;
}


2)
#include <stdio.h>
int main(void)
{
    int a, b, c;
    scanf("%d %d", &a, &b);
 
    c = b + 100 - a;
 
    printf("%d\n", c);
    if (c > 0)
        printf("Obesity");
 
    return 0;   
}


3)
#include <stdio.h>
int main(void)
{
    int a;
    scanf("%d", &a);
 
    if (a >= 20)
        printf("adult");
    else
        printf("%d years later", 20 - a);
 
    return 0;
}


4)
#include <stdio.h>
int main(void)
{
    double a;
    scanf("%lf", &a);
 
    if (a <= 50.8)
        printf("Flyweight\n");
    else if (a < 61.23)
        printf("Lightweight\n");
    else if (a < 72.57)
        printf("Middleweight\n");
    else if (a < 88.45)
        printf("Cruiserweight\n");
    else
        printf("Heavyweight\n");
 
    return 0;
}


5)
#include <stdio.h>
int main(void)
{
    double a, b;
    scanf("%lf %lf", &a, &b);
 
    if (a >= 4.0 && b >= 4.0)
        printf("A");
    else if (a >= 3.0 && b >= 3.0)
        printf("B");
    else
        printf("C");
        
    return 0;
}


6)
#include <stdio.h>
int main(void)
{
    char ch;
    int a;
    scanf("%c %d", &ch, sizeof(ch), &a);
 
    if (ch == 'F') {
        if (a >= 18)
            printf("MAN");
        else
            printf("BOY");
    }
    else if (ch == 'M') {
        if (a >= 18)
            printf("WOMAN");
        else
            printf("GIRL");
    }
    return 0;
}


7)
#include <stdio.h>
int main(void)
{
    char ch;
    scanf("%c", &ch, sizeof(ch));
 
    switch (ch)
    {
    case 'A':
        printf("Excellent");
        break;
    case 'B':
        printf("Good");
        break;
    case 'C':
        printf("Usually");
        break;
    case 'D':
        printf("Effort");
        break;
    case 'F':
        printf("Failure");
        break;
    default:
        printf("error");
        break;
    }
    return 0;
}

8)
#include <stdio.h>
int main(void)
{
    double a;
    scanf("%lf", &a);
 
    switch ((int)a)
    {
    case 4:
        printf("scholarship");
        break;
    case 3:
        printf("next semester");
        break;
    case 2:
        printf("seasonal semester");
        break;
    default:
        printf("retake");
        break;
    }
    return 0;
}


9)
#include <stdio.h>
int main(void)
{
    int a, b, c, min;
    scanf("%d %d %d", &a, &b, &c);
 
    min = (a < b) ? a : b;
    min = (min < c) ? min : c;
 
    printf("%d", min);
 
    return 0;
}


**형성평가**
1)
#include <stdio.h>
int main(void)
{
    int a, b;
    scanf("%d %d", &a, &b);
 
    if (a > b)
        printf("%d", a - b);
    else
        printf("%d", b - a);
 
    return 0;
}


2)
#include <stdio.h>
int main(void)
{
    int a;
    scanf("%d", &a);
 
    if (a == 0)
        printf("zero");
    else if (a > 0)
        printf("plus");
    else
        printf("minus");
 
    return 0;
}


3)
#include <stdio.h>
int main(void)
{
    int a;
    scanf("%d", &a);
 
    if (a % 400 == 0 || a % 4 == 0 && a % 100 != 0)
    {
        printf("leap year");
    }
    else
        printf("common year");
 
    return 0;
}


4)
#include <stdio.h>
int main(void)
{
    int a;
    printf("Number? ");
    scanf("%d", &a);
 
    if (a == 1)
        printf("dog");
    else if (a == 2)
        printf("cat");
    else if (a == 3)
        printf("chick");
    else
        printf("I don't know.");
        
    return 0;
}


5)
#include <stdio.h>
int main(void)
{
    int a;
    scanf("%d", &a);
    if (a == 2)
    {
        printf("%d", 28);
    }
    else if (a == 4 || a == 6 || a == 9 || a == 11)
    {
        printf("%d", 30);
    }
    else
    {
        printf("%d", 31);
    }
    return 0;
}
