**자가진단**
1)
#include <stdio.h>
int main(void)
{
    int a, b, c;
 
    scanf("%d %d %d", &a, &b, &c);
 
    printf("sum : %d\n", a + b + c);
    printf("avg : %d", (a + b + c) / 3);
 
    return 0;
}


2)
#include <stdio.h>
int main(void)
{
    int a, b;
 
    scanf("%d %d", &a, &b);
 
    printf("%d ", a + 100);
    printf("%d", b % 10);
 
    return 0;
}


3)
#include <stdio.h>
int main(void)
{
    int a;
 
    scanf("%d", &a);
 
    printf("%d\n", a++);
    printf("%d", ++a);
 
    return 0;
}


4)
#include <stdio.h>
int main(void)
{
    int a, b, c;
 
    scanf("%d %d", &a, &b);
 
    --b;
    c = a * b;
 
    printf("%d %d %d", ++a, b, c);
 
    return 0;
}


5)
#include <stdio.h>
int main(void)
{
    int a, b;
    scanf("%d %d", &a, &b);
 
    a == b ? printf("1\n") : printf("0\n");
    a != b ? printf("1\n") : printf("0\n");
 
    return 0;
}


6)
#include <stdio.h>
int main(void)
{
    int a, b;
    scanf("%d %d", &a, &b);
 
    printf("%d > %d --- %d\n", a, b, a > b);
    printf("%d < %d --- %d\n", a, b, a < b);
    printf("%d >= %d --- %d\n", a, b, a >= b);
    printf("%d <= %d --- %d\n", a, b, a <= b);
 
 
    return 0;
}


7)
#include <stdio.h>
int main(void)
{
    int a, b;
    scanf("%d %d", &a, &b);
 
    printf("%d %d", a && b, a || b);
 
    return 0;
}


8)
#include <stdio.h>
int main(void)
{
    int a, b, c;
    scanf("%d %d %d", &a, &b, &c);
 
    printf("%d %d", a > b && a > c, a == b && b == c);
 
    return 0;
}


**형성평가**
1)
#include <stdio.h>
int main(void)
{
    int a, b, c, d;
    int sum;
    scanf("%d %d %d %d", &a, &b, &c, &d);
 
    sum = a + b + c + d;
    printf("sum %d\navg %d\n\n", sum, sum / 4);
 
    return 0;
}


2)
#include <stdio.h>
int main(void)
{
    int a, v;
    scanf("%d %d", &a, &v);
 
    printf("%d / %d = %d...%d", a, v, a / v, a % v);
 
    return 0;
}


3)
#include <stdio.h>
int main(void)
{
    int a, v;
    scanf("%d %d", &a, &v);
 
    printf("width = %d\nlength = %d\narea = %d\n\n", a + 5, v * 2, (a + 5) * (v * 2));
 
    return 0;
}


4)
#include <stdio.h>
int main(void)
{
    int a, b;
    scanf("%d %d", &a, &b);
 
    printf("%d %d\n", ++a, b--);
    printf("%d %d\n\n", a, b);
    
    return 0;
}


5)
#include <stdio.h>
int main(void)
{
    int a, b, c, d;
    scanf("%d %d", &a, &b);
    scanf("%d %d", &c, &d);
 
    printf("%d\n\n", a > c && b > d);
 
    return 0;
}
