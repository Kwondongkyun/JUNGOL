**자가진단**
1)
#include <stdio.h>
int main(void)
{
    double a, b;
    int c;
    scanf("%lf %lf", &a, &b);
 
    printf("%d %d", (int)(a * b), (int)(a)*(int)(b));
 
    return 0;
}


2)
#include <stdio.h>
int main(void)
{
    int a, b;
    scanf("%d %d", &a, &b);
 
    printf("%d %.2lf", a / b, (double)(a) / b);
 
    return 0;
}


**형성평가**
1)
#include <stdio.h>
int main(void)
{
    int a, b, c;
    scanf("%d %d %d", &a, &b, &c);
 
    printf("%.1lf", ((double)a + (double)b + (double)c) / 3);
 
    return 0;
}


2)
#include <stdio.h>
int main(void)
{
    double a, b, c;
    scanf("%lf %lf %lf", &a, &b, &c);
 
    printf("sum %d\navg %d", (int)a + (int)b + (int)c, 
        (int)(a + b + c) / 3);
 
    return 0;
}


3)
#include <stdio.h>
int main(void)
{
    int a = 5;
    a += 10;
    a = a - 1;
    printf("%d\n", a + 1);
 
    return 0;
}--);
    printf("%d %d\n\n", a, b);
    
    return 0;
}


